    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "electron-forge start",
    "typecheck": "tsc --noEmit",
    "electron:dev": "npm run electron:build && electron dist/electron/main.js",
    "electron:build": "tsc -p src/electron/tsconfig.json && npm run electron:build:preload",
    "electron:build:preload": "tsc -p src/electron/tsconfig.preload.json",
    "electron:start": "electron-forge start",
    "electron:make": "npm run electron:build && npm run package",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
